{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"outDir": "./dist",
		"rootDir": "./",
		"strict": true,
		"moduleResolution": "node",
		"typeRoots": ["./src/@types", "./node_modules/@types"],
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
